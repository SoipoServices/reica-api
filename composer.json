{
    "name": "soiposervices/reica",
    "description": "This package provides a seamless interface to interact with the getreica.com API, enabling developers to access its powerful image generation and manipulation features. Designed with ease of use in mind, this client simplifies integration into your applications, allowing you to leverage getreicaâ€™s capabilities for creating realistic images tailored to your needs.",
    "keywords": [
        "soiposervices",
        "reica"
    ],
    "homepage": "https://github.com/soiposervices/reica",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Luigi Laezza",
            "email": "luigi@soiposervices.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0",
        "saloonphp/saloon": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "pestphp/pest": "^1.23",
        "pestphp/pest-plugin-laravel": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Soiposervices\\Reica\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Soiposervices\\Reica\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Soiposervices\\Reica\\ReicaServiceProvider"
            ],
            "aliases": {
                "Reica": "Soiposervices\\Reica\\ReicaFacade"
            }
        }
    }
}
